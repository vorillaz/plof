{
  "name": "ploff",
  "version": "0.0.1",
  "description": "A small utility to grab files from a git repository",
  "packageManager": "pnpm@8.7.0",
  "homepage": "https://github.com/vorillaz/ploff#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/vorillaz/ploff.git"
  },
  "type": "module",
  "bin": {
    "ploff": "./dist/cli.cjs"
  },
  "keywords": [
    "utility",
    "git",
    "pull",
    "clone",
    "git clone"
  ],
  "scripts": {
    "start": "npm run dev",
    "build": "tsup",
    "semantic-release": "semantic-release",
    "test": "vitest --test-timeout=50000"
  },
  "dependencies": {
    "chalk": "5.3.0",
    "commander": "11.1.0",
    "enquirer": "2.4.1",
    "shell-exec": "1.1.2",
    "ora": "7.0.1"
  },
  "devDependencies": {
    "@semantic-release/release-notes-generator": "12.1.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "9.2.5",
    "@semantic-release/npm": "^11.0.2",
    "tsup": "8.0.1",
    "prettier": "3.1.1",
    "semantic-release": "^22.0.12",
    "typescript": "5.3.3",
    "vitest": "0.34.6"
  },
  "author": "Theodore Vorillas<info@vorillaz.com>",
  "license": "MIT"
}
